<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.newfoundsoftware.pos.CategoryController">
   <children>
      <Pane prefHeight="13.0" prefWidth="600.0" style="-fx-background-color: #0097e6;" />
      <Label layoutX="14.0" layoutY="35.0" text="Category" />
      <TextField fx:id="tfCategoryName" layoutX="86.0" layoutY="31.0" prefHeight="32.0" />
      <Button fx:id="btnSave" layoutX="243.0" layoutY="29.0" mnemonicParsing="false" onAction="#saveCategory" prefHeight="32.0" prefWidth="102.0" style="-fx-background-color: #26c6da;" text="Save" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="btnUpdate" disable="true" layoutX="351.0" layoutY="29.0" mnemonicParsing="false" onAction="#editEntry" prefHeight="32.0" prefWidth="102.0" style="-fx-background-color: #26c6da;" text="Update" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="btnDelete" disable="true" layoutX="458.0" layoutY="29.0" mnemonicParsing="false" onAction="#deleteEntry" prefHeight="32.0" prefWidth="102.0" style="-fx-background-color: #26c6da;" text="Delete" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <TableView fx:id="tableCategory" layoutX="14.0" layoutY="77.0" prefHeight="311.0" prefWidth="571.0">
         <columns>
            <TableColumn fx:id="colId" prefWidth="75.0" text="Id" />
            <TableColumn fx:id="colName" prefWidth="75.0" text="Name" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
